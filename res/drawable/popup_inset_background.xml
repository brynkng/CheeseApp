<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="#00000000" />


        </shape>
    </item>

    <item android:right="1dp" android:bottom="1dp">
        <shape
                android:shape="rectangle">
            <stroke android:width="3dp" android:color="#FFDDDDDD" />
            <solid android:color="#cccccc" />
            <corners android:radius="5dp" />
        </shape>
    </item>

    <item android:bottom="2dp" android:top="2dp" android:left="2dp" android:right="2dp" >
        <shape
                android:shape="rectangle">
            <stroke android:width="1dp" android:color="#666666" />
            <solid android:color="#00000000" />
            <corners android:radius="5dp" />
        </shape>

    </item>


</layer-list>
